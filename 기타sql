insert into foodtbl values( '돈까스', '12000');
insert into foodtbl values( '마라탕', '12000');
insert into foodtbl values( '덮밥', '12000');
commit;

create table musictbl(
	singer varchar2(20) primary key ,
	title varchar2(20)
);

