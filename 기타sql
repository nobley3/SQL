create table goodstbl(

	id varchar2(20) primary key,
	name varchar2(20) ,
	price number(4)
);

insert into goodstbl values('c001','아메리카노',2000);
insert into goodstbl values('c002','바닐라라떼',2800);
insert into goodstbl values('c003','연유라떼',3800);

select * from GOODSTBL;
commit;

create   table  foodtbl(
   name varchar2(20) primary key, 
   price varchar2(10)
);


insert into foodtbl values( '스파게티', '10000');
